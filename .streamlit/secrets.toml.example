# Streamlit Secrets Configuration Example
# Copy this file to secrets.toml and fill in your actual values
# NEVER commit secrets.toml to git!

[aws]
region = "us-east-1"
access_key_id = "YOUR_AWS_ACCESS_KEY_ID"
secret_access_key = "YOUR_AWS_SECRET_ACCESS_KEY"

# Optional: Solo necesario si usas credenciales temporales (AWS SSO, AssumeRole, etc.)
# session_token = "YOUR_AWS_SESSION_TOKEN"

[agentcore]
